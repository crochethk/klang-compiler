/**
 * Tries out:
 *      - fibonacci
 */

fn fib(n: i64) -> i64 {
    return n<=1 ? n : fib(n-1)+fib(n-2);
    // if n<=1 {return n;}
    // else {
    //     return fib(n-1)+fib(n-2);
    // }
}

fn ___main___() -> i64 {
    return fib(7); // -> 13
}


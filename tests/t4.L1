/**
 * Tries out:
 *     - (nested) if-else
 *     - nested returns
 */
fn max(a: int, b: int): int {
    return a >=b ? a : b;
}

fn max3(a: int, b: int, c: int): int {
    if max(a,b) >= max(b,c) {
        return max(a,b);
    } else {
        if b >=c {
            return b;
        } else {
            return c;
        }
    }
}

